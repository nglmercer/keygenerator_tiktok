{
  "name": "keygenerator",
  "description": "TikTok Stream Key Generator - Electron application for generating TikTok stream keys via Streamlabs API integration.",
  "author": "KeyGenerator Team",
  "module": "index.ts",
  "main": "main.js",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "bun run typecheck && bun run scripts/build.ts",
    "build:bundle": "bun run typecheck && bun run scripts/build.ts",
    "build:dist": "bun run build:bundle && npx electron-builder",
    "build:dist:linux": "bun run build:bundle && npx electron-builder --linux",
    "build:dist:mac": "bun run build:bundle && npx electron-builder --mac",
    "build:dist:win": "bun run build:bundle && npx electron-builder --win",
    "typecheck": "bunx tsc --noEmit",
    "discover": "bun scripts/discover.ts",
    "start": "bun run build:bundle && electron ."
  },
  "devDependencies": {
    "@types/bun": "latest",
    "electron-builder": "^26.7.0",
    "electron": "^39.2.7"

  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "ts-node": "^10.9.2"
  }
}