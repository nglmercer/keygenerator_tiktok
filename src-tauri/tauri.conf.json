{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "keygenerator",
  "version": "0.1.0",
  "identifier": "com.keygenerator.tiktok",
  "build": {
    "frontendDist": "../src/ui",
    "devUrl": "http://localhost:1420"
  },
  "app": {
    "withGlobalTauri": true,
    "security": {
      "csp": null,
      "devCsp": null
    },
    "windows": [
      {
        "title": "TikTok Stream Key Generator",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all"
  }
}
